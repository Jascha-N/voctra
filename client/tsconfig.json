{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "moduleResolution": "node",
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strict": true,
        "noUnusedLocals": true
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ]
}
